title: <%= link_to " #{streak.title}", "streak/#{streak.id}" %>
<%=" | " + "aim: #{streak.aim}" + " | " %>
<%= "created at: #{streak.created_at}"  + " | "  %>
<%= streak.frequency %>
<%= form_for streak, url: '/streaks/update'  do |f| %>
  <%= f.hidden_field :frequency, value: (streak.frequency + 1) %>
  <%= f.submit :update %>
  
<% end %>